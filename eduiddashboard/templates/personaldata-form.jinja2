 <div class='profile-form'>
   {% for message in request.session.pop_flash(queue='forms') %}
     <div class="alert alert-{{ message|get_flash_message_type }}">
       <button type="button" class="close" data-dismiss="alert">&times;</button>
       {{ message }}
     </div>
   {% endfor %}

   <div class='intro'>
   {% trans %}This is your personal info{% endtrans %}
   </div>
   
   {{ form.render(person)|safe }}
</div>
